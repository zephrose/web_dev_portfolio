<%- include("partials/header") -%>

<h1>Home</h1>

<a class="btn btn-primary" href="/compose" role="new_post">New Post?</a><br/><br/>

<p> <%= homeCont %></p>

<div>
	<div class="posts">
	<% posts.forEach(function(post) { %>	
		<a href="/post/<%= post._id %>"><h3 class="title"><%= post.title %></h3></a>
		<p class="post text-truncate-overflow" ><%= post.post %></p>
	<% }); %>
	</div>		
</div>

<%- include("partials/footer") -%>
